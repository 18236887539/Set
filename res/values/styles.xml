<resources>

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="MD">
        <item name="animationVelocity">6</item>
        <item name="insetBottom">5dp</item>
        <item name="insetTop">5dp</item>
        <item name="insetLeft">5dp</item>
        <item name="insetRight">5dp</item>
        <item name="measureFactor">1.8</item>
        <item name="offDrawable">@drawable/md_back_off</item>
        <item name="onDrawable">@drawable/md_back_on</item>
        <item name="thumbDrawable">@drawable/md_thumb</item>
    </style>

</resources>